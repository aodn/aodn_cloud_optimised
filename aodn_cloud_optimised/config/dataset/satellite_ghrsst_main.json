{
  "dataset_name": "srs_ghrsst_main",
  "logger_name": "srs_ghrsst_main",
  "cloud_optimised_format": "zarr",
  "metadata_uuid": null,
  "schema": {
    "lon": {
      "type": "float32"
    },
    "lat": {
      "type": "float32"
    },
    "time": {
      "type": "datetime64[ns]"
    },
    "sea_surface_temperature": {
      "type": "float32"
    },
    "sea_surface_temperature_day_night": {
      "type": "float32",
      "drop_var": true
    },
    "sst_dtime": {
      "type": "float64"
    },
    "dt_analysis": {
      "type": "float32"
    },
    "wind_speed": {
      "type": "float32",
      "drop_var": true
    },
    "wind_speed_dtime_from_sst": {
      "type": "float32",
      "drop_var": true
    },
    "sea_ice_fraction": {
      "type": "float32",
      "drop_var": true
    },
    "sea_ice_fraction_dtime_from_sst": {
      "type": "float32",
      "drop_var": true
    },
    "satellite_zenith_angle": {
      "type": "float32"
    },
    "l2p_flags": {
      "type": "float32"
    },
    "quality_level": {
      "type": "float32"
    },
    "sses_bias": {
      "type": "float32"
    },
    "sses_standard_deviation": {
      "type": "float32"
    },
    "sses_count": {
      "type": "float32"
    },
    "sst_count": {
      "type": "float32"
    },
    "sst_mean": {
      "type": "float32"
    },
    "sst_standard_deviation": {
      "type": "float32"
    },
    "adi_dtime_from_sst": {
      "type": "float32",
      "drop_var": true
    },
    "aerosol_dynamic_indicator": {
      "type": "float32",
      "drop_var": true
    }
  },
  "run_settings": {
    "coiled_cluster_options": {
      "n_workers": [
        60,
        220
      ],
      "scheduler_vm_types": "m7i.2xlarge",
      "worker_vm_types": "m7i.2xlarge",
      "allow_ingress_from": "me",
      "compute_purchase_option": "spot_with_fallback",
      "worker_options": {
        "nthreads": 8,
        "memory_limit": "32GB"
      }
    },
    "batch_size": 100,
    "cluster": {
      "mode": "coiled",
      "restart_every_path": false
    },
    "paths": [
      {
        "s3_uri": "s3://imos-data/IMOS/",
        "filter": [],
        "year_range": []
      }
    ],
    "clear_existing_data": true,
    "raise_error": false
  },
  "schema_transformation": {
    "global_attributes": {
      "set": {
        "title": "Temperature logger"
      }
    },
    "var_template_shape": "sea_surface_temperature",
    "vars_incompatible_with_region": [
      "lat",
      "lon"
    ],
    "dimensions": {
      "time": {
        "name": "time",
        "chunk": 5,
        "rechunk": true,
        "append_dim": true
      },
      "latitude": {
        "name": "lat",
        "chunk": 500
      },
      "longitude": {
        "name": "lon",
        "chunk": 500
      }
    }
  }
}
